<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Smoke Suite">
	<listeners>
		<listener class-name="com.HRMS.testbase.Listener"></listener>
	</listeners>
	<test thread-count="5" name="Smoke Test">
		<groups>
			<run>
				<include name="smoke"></include>
			</run>
		</groups>

		<packages>
			<package name="com.HRMS.testcases"></package>
		</packages>
	</test> <!-- Test -->
</suite> <!-- Suite -->
